# Base URL of the backend server
VITE_API_BASE_URL=http://localhost:8000

# URL path prefix appended to VITE_API_BASE_URL for all API calls (e.g. /api/v1).
VITE_API_URL_PREFIX=/api/v1

# Maximum time (in milliseconds) to wait for an API response before the request is cancelled.
VITE_API_TIMEOUT=3000

# Minimum severity level for log messages printed to the browser console.
# Accepted values (from most to least verbose): debug | info | warn | error
VITE_LOG_LEVEL=debug